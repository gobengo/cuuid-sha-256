{
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "declarationMap": true,
    "incremental": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "checkJs": true,
    "allowJs": true,
    "outDir": "lib",
    "emitDeclarationOnly": true,
    "allowImportingTsExtensions": true,
    "lib": [
      "ESNext",
    ]
  },
  "include": [
    "*.ts",
    "*.tsx"
  ],
  "references": [
  ]
}
